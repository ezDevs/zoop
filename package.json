{
    "name": "zoop-integration",
    "version": "0.0.3",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/coreh/zoop.git"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "dependencies": {
        "@types/qs": "^6.5.3",
        "fetch-ponyfill": "^6.0.2",
        "isomorphic-base64": "^1.0.2",
        "qs": "^6.7.0",
        "tslib": "^1.9.3"
    },
    "devDependencies": {
        "@types/jest": "^24.0.11",
        "jest": "^24.7.1",
        "ts-jest": "^24.0.2",
        "typescript": "^3.4.5",
        "tslint": "^5.16.0"
    },
    "scripts": {
        "prepare": "tsc --project . --declaration --outDir dist/",
        "clean": "rm -f *.js *.d.ts",
        "lint": "tslint -c tslint.json '**/*.ts' --exclude '**/node_modules/**'",
        "test": "jest",
        "coverage": "jest --coverage"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
